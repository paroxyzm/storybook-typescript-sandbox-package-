{
  "name": "storybook-typescript-sandbox-package",
  "version": "0.2.0",
  "description": "This is a testing repo/package for storybook for react in typescript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "npm run storybook",
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "version": "git add -A src"
  },
  "files": [
    "lib/**/*"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "keywords": [
    "poc",
    "storybook"
  ],
  "author": "lmi",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paroxyzm/storybook-typescript-sandbox-package.git"
  },
  "bugs": {
    "url": "https://github.com/paroxyzm/storybook-typescript-sandbox-package/issues"
  },
  "homepage": "https://github.com/paroxyzm/storybook-typescript-sandbox-package",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/react": "^5.3.18",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/storybook__react": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "prettier": "^2.0.4",
    "ts-jest": "^25.4.0",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
  }
}
